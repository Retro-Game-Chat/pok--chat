{
  "name": "poke--chat",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:server": "nodemon ./bin/www",
    "dev:client": "cd client && npm run serve",
    "dev": "concurrently --kill-others-on-fail 'npm run dev:server' 'npm run dev:client'",
    "start": "node ./bin/www",
    "postinstall": "cd client && npm install"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "nexmo": "^2.7.0-beta-1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
